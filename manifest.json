{
	"manifest_version": 2,
	"name": "wiki-peeks",
	"description": "a sample manifest",
	"version": "1.0",
	"permissions": [
		"storage"
	],
	"icons": {
		"default_icon": "snowcappedmountains.svg"
	},
	"background": {
		"scripts": ["event.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "snowcappedmountains.svg",
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"web_accessible_resources": [
		"snowcappedmountains.svg"
	],
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
