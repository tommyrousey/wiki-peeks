{
	"manifest_version": 2,
	"name": "wiki-peeks",
	"description": "a sample manifest",
	"version": "1.0",
	"background": {
		"page": "background.html",
		"persistent": true
	},
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
	"options_page": "options.html",
	"browser_action": {
		"default_popup": "popup.html"
	},
	"content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
	"permissions": [
		"tabs",
        "activeTab",
		"http://*/*",
		"https://*/*",
		"storage",
		"contextMenus"
	]
}
