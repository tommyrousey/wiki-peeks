{
	"name": "wiki-peeks",
	"version": "1.0",
	"manifest_version": 2,
	"description": "peeks at wikis!",
	"browser_action": {
		"default_icon": "snowcappedmountains.svg"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"],
			"js": ["content.js"]
		}
	],
	"background": {
		"scripts": ["background.js"]
	}
}
